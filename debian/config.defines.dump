[('abi',)]
abiname: '4.slh.3'

[('base',)]
arches: ['amd64', 'i386']
featuresets: ['none']
compiler: 'gcc-4.4'

[('base', 'amd64')]
kernel-arch: 'x86'
featuresets: ['none']

[('base', 'amd64', 'none')]
implicit-flavour: True
flavours: ['sidux-amd64']

[('base', 'i386')]
kernel-arch: 'x86'
featuresets: ['none']

[('base', 'i386', 'none')]
implicit-flavour: True
flavours: ['sidux-686']

[('commands-image-initramfs-generators',)]
initramfs-tools: 'update-initramfs'

[('description', 'amd64', None, 'sidux-amd64')]
hardware: '64-bit PCs'
hardware-long: 'PCs with AMD64 or Intel 64 processors'

[('description', 'i386', None, 'sidux-686')]
hardware: 'modern PCs'
hardware-long: 'PCs with Intel Pentium Pro/II/III/4/4M/D/M, Xeon, Celeron, Core or Atom; AMD K6, Geode LX/NX, Athlon (K7), Duron, Opteron, Sempron, Turion or Phenom; Transmeta Efficeon; VIA C3 "Nehemiah" or C7 processors'

[('image',)]
initramfs-generators: ['initramfs-tools', 'initramfs-fallback']
type: 'plain'

[('image', 'amd64')]
suggests: 'grub2 | grub-gfxboot | grub-legacy, crda'
configs: ['kernelarch-x86/config']

[('image', 'amd64', None, 'sidux-amd64')]
configs: ['kernelarch-x86/config-arch-64']

[('image', 'i386')]
suggests: 'grub2 | grub-gfxboot | grub-legacy, crda'
configs: ['kernelarch-x86/config']

[('image', 'i386', None, 'sidux-686')]
configs: ['kernelarch-x86/config-arch-32']

[('relations',)]
initramfs-fallback: 'linux-initramfs-tool'
initramfs-tools: 'initramfs-tools (>= 0.93)'
gcc-4.4: 'gcc-4.4'

[('version',)]
abiname: '-4.slh.3'
source: '2.6.33-50'

