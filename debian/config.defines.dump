[('abi',)]
abiname: '3.slh.3'

[('aptosid-amd64-dbg', 'amd64')]
enabled: 'false'

[('base',)]
arches: ['amd64', 'i386']
featuresets: ['none']
compiler: 'gcc-4.6'

[('base', 'amd64')]
kernel-arch: 'x86'
featuresets: ['none']

[('base', 'amd64', 'none')]
implicit-flavour: True
flavours: ['aptosid-amd64']

[('base', 'i386')]
kernel-arch: 'x86'
featuresets: ['none']

[('base', 'i386', 'none')]
implicit-flavour: True
flavours: ['aptosid-686']

[('description', 'amd64', None, 'aptosid-amd64')]
hardware: '64-bit PCs'
hardware-long: 'PCs with AMD64 or Intel 64 processors'

[('description', 'i386', None, 'aptosid-686')]
hardware: 'modern PCs'
hardware-long: 'PCs with Intel Pentium Pro/II/III/4/4M/D/M, Xeon, Celeron, Core or Atom; AMD Geode LX/NX, Athlon (K7), Duron, Opteron, Sempron, Turion or Phenom; Transmeta Efficeon; VIA C3 "Nehemiah" or C7 processors'

[('image',)]
initramfs-generators: ['initramfs-tools', 'initramfs-fallback']
type: 'plain'

[('image', 'amd64')]
suggests: 'crda'
configs: ['kernelarch-x86/config']
bootloaders: ['grub-pc', 'extlinux']

[('image', 'amd64', None, 'aptosid-amd64')]
configs: ['kernelarch-x86/config-arch-64']

[('image', 'i386')]
suggests: 'crda'
configs: ['kernelarch-x86/config']
bootloaders: ['grub-pc', 'extlinux']

[('image', 'i386', None, 'aptosid-686')]
configs: ['kernelarch-x86/config-arch-32']
recommends: 'libc6-i686'

[('image-dbg', 'i386', None, 'aptosid-686')]
enabled: False

[('relations',)]
initramfs-fallback: 'linux-initramfs-tool'
lilo: 'lilo (>= 22.8-8.2~)'
initramfs-tools: 'initramfs-tools (>= 0.99)'
gcc-4.6: 'gcc-4.6'
elilo: 'elilo (>= 3.12-3.1~)'

[('version',)]
abiname: '-3.slh.3'
source: '2.6.39-32'

