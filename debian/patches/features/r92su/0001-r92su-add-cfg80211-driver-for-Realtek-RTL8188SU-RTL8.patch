+static netdev_tx_t r92su_start_xmit(struct sk_buff *skb,
+				    struct net_device *ndev)
+{
+	struct r92su *r92su = ndev->ml_priv;
+	r92su_tx(r92su, skb);
+	return NETDEV_TX_OK;
+}
