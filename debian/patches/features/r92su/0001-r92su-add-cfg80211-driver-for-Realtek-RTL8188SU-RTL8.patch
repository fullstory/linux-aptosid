+static void *devm_dup(struct device *dev, void *src, size_t len)
+{
+	void *tmp;
+
+	tmp = devm_kzalloc(dev, len, GFP_KERNEL);
+	if (tmp)
+		memcpy(tmp, src, len);
+	return tmp;
+}
